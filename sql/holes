CREATE TABLE IF NOT EXISTS `golfDB`.`holes` (
  `courseID` TINYINT(3) NOT NULL,
  `holeID` TINYINT(2) NOT NULL,
  `name` VARCHAR(45) NOT NULL,
  `par` TINYINT(2) NOT NULL,
  `stroke` TINYINT(2) NOT NULL,
  `meters` TINYINT(3) NOT NULL,
  PRIMARY KEY (`holeID`),
  CONSTRAINT `courseID`
    FOREIGN KEY (`courseID`)
    REFERENCES `golfDB`.`courses` (`courseID`)
    ON DELETE CASCADE
    ON UPDATE CASCADE)
ENGINE = InnoDB
